{
  "include": ["src", "public", "app", ".next/types/**/*.ts"],
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "preserve",

    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
